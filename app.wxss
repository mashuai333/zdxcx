/**app.wxss**/
@import "/lib/style/iconfont.wxss";
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
page{
  background: #f5f9fa;
  position: relative;
}

.tab-bar{
  z-index: 999;
  position: fixed;
  display: flex;
  width: 100%;
  font-size: 24rpx;
  bottom:0;
  border-top:1px solid #daebe5;
}
.menu-item{
  flex:1;
  text-align: center;
  padding: 4px 0;
}
.img{
  width: 48rpx;
  height: 48rpx;
  display: block;
  margin:auto;
}
.clear{
  clear: both;
}
